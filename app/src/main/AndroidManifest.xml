<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pl.nataliana.musicalapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/note"
        android:label="@string/app_name"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PlaylistActivity"
            android:label="@string/playlist" />
        <activity
            android:name=".SongActivity"
            android:label="@string/song" />
        <activity
            android:name=".TrendingActivity"
            android:label="@string/trending" />
        <activity
            android:name=".PremiumActivity"
            android:label="@string/premium"></activity>
    </application>
</manifest>